@model EditMetaViewModel
@{
    ViewBag.Title = "新建售后原因";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="vw-meta-form vw-new-shouhoureason" data-page-js="newshouhoureason">
    @Html.Partial("_Nav")
    <h2>@ViewBag.Title</h2>
    <div class="form" validation-group="submit" autocomplete="off">
        <fieldset>
            <div class="row">
                <div class="form-group col-6">
                    <label for="TxtName">名称</label>
                    <input type="password" style="display: none;" />
                    <input type="text" class="form-control" maxlength="50" name="Name" id="TxtName" autocomplete="off" />
                    <span class="validator" id="vdName" data-required="" data-validate-target="#TxtName"><span class="error-message text-danger">此项必填</span></span>
                </div>
                <div class="form-group col-6">
                    <label for="TxtSN">SN</label>
                    <input type="number" class="form-control" name="SN" id="TxtUserSN" autocomplete="off" value="0" step="1" />
                    <span class="validator" id="vdSN" data-required="" data-validate-target="#TxtUserSN"><span class="error-message text-danger">此项必填</span></span>
                </div>
            </div>
        </fieldset>
        <div class="form-group">
            <button type="button" class="btn btn-primary" id="BtnSubmit">提交</button>
        </div>
    </div>
</div>
